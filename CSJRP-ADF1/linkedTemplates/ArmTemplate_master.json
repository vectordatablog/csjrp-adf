{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "CSJRP-ADF1"
		},
		"OnpremSQLServer_ls_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "onpremsqlconn"
		},
		"csjrp_dbricks_ls_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0304-002117-1eobtv1e"
		},
		"csjrp_kv_ls_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://csjrp-kv1.vault.azure.net/"
		},
		"storage_adls_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://csjrpproject1.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "CSJRP-ADF1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"csjrp_dbricks_ls_properties_typeProperties_existingClusterId": {
						"value": "[parameters('csjrp_dbricks_ls_properties_typeProperties_existingClusterId')]"
					},
					"csjrp_kv_ls_properties_typeProperties_baseUrl": {
						"value": "[parameters('csjrp_kv_ls_properties_typeProperties_baseUrl')]"
					},
					"storage_adls_ls_properties_typeProperties_url": {
						"value": "[parameters('storage_adls_ls_properties_typeProperties_url')]"
					},
					"OnpremSQLServer_ls_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('OnpremSQLServer_ls_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}